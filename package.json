    "babel-cli": "6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "6.26.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "debug-levels": "^0.2.0",
    "detect-node": "^2.0.3",
    "fast-deep-equal": "^1.0.0",
    "is-valid-path": "^0.1.1",
